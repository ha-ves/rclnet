<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Target Name="ImportRos2cs" 
			BeforeTargets="GenerateInterfaces_ros2cs">

		<MSBuild Projects="$(MSBuildProjectDirectory)\..\ros2cs\ros2cs.csproj"
		  		 Targets="Publish"
		  		 Properties="Configuration=$(Configuration);TargetFramework=net6.0;Version=$(Version)"
				/>

		<PropertyGroup>
			<Ros2csDir>$(MSBuildProjectDirectory)\..\ros2cs\bin\$(Configuration)\net6.0\publish</Ros2csDir>
			<Ros2csExe>$(Ros2csDir)\ros2cs.dll</Ros2csExe>
		</PropertyGroup>
	</Target>
</Project>